{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "dmsanthosh/multi-client:4",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "dmsanthosh/multi-server:4",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "dmsanthosh/multi-worker:4",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "dmsanthosh/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links":["client", "server"]
    }
  ]
}